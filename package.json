    "camelcase-keys": "^7.0.1",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
